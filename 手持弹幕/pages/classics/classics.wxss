        width: 1084rpx;
    }
}

.hide {
    display: none;
}

.select text {
    display: inline-block;
    vertical-align: 4rpx;
}

.select image {
    display: inline-block;
    width: 56rpx;
    height: 28rpx;
}

.scorll-text text {
    margin-right: 150pt;
}

.scorll-text text:last-child {
    margin-right: 0;
}

.panel {
    text-align: center;
}

.panel-scroll {
    display: inline-flex;
    white-space: nowrap;
    transition: all .5s;
    padding-right: 32rpx;
}

.video-bg .accomplish-btn-group-box {
    margin-top: -40rpx;
}

.panel-scroll::-webkit-scrollbar {
    display: none;
    height: 0;
    color: transparent;
    width: 0!important;
}

button {
    border: none;
}

.relative {
    position: relative;
    display: inline-block;
    margin-top: 40rpx;
}

.panel-scroll button {
    display: inline-block;
    font-size: 24rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 5px;
    text-align: center;
    line-height: 80rpx;
    background-color: transparent;
    margin-right: 16px;
    box-sizing: border-box;
    color: #fff;
    background: #262628;
    padding: 0;
    border: 2rpx solid #262628;
}

.color button,.panel-scroll button::after {
    border: none;
}

.panel-scroll image {
    display: block;
    width: 78rpx;
    height: 78rpx;
}

.video-lock {
    position: absolute;
    overflow: hidden;
    height: 32rpx;
    color: #fff;
    line-height: 32rpx;
    font-size: 22rpx;
    text-align: center;
    background-color: #ff0071;
    right: 16rpx;
    top: -16rpx;
    z-index: 9;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.video-lock,.video-lock image {
    display: block;
    width: 40rpx;
}