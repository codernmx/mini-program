page {
    padding: 0;
    margin: 0;
    font-size: 30rpx;
    color: #757575;
    background-color: #f8f8f8;
}

.oneImage {
    width: 210rpx;
    height: 370rpx;
    font-size: 28rpx;
}

.preview-image {
    display: block;
    box-sizing: border-box;
    width: 210rpx;
    height: 370rpx;
    overflow: hidden;
    margin-top: 30rpx;
    margin-left: 30rpx;
    float: left;
    position: relative;
    border-radius: 10rpx;
    border: solid 1px #f0f0f0;
}

.select-image {
    background-color: #fb7299;
    color: #ffffff;
    text-align: center;
    line-height: 370rpx;
}

.remove {
    position: absolute;
    top: 0;
    right: 0;
    font-size: 24rpx;
    width: 80rpx;
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    box-sizing: border-box;
    background-color: #FC8E94;
    color: #ffffff;
    text-align: center;
    box-shadow: -4rpx 4rpx 10rpx rgba(0,0,0,0.2);
}

.bottom-tab {
    position: fixed;
    z-index: 99;
    bottom: 0;
    left: 0;
    right: 0;
    margin: 0 auto;
    font-size: 30rpx;
    width: 750rpx;
    overflow: hidden;
    box-sizing: border-box;
    color: #ffffff;
    display: flex;
    justify-content: center;
    box-shadow: 0rpx -4rpx 10rpx rgba(0,0,0,0.2);
}

.tabBtn {
    width: 50%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    float: left;
}

.tabBtn:nth-child(1) {
    background-color: #FC8E94;
}

.tabBtn:nth-child(2) {
    background-color: #f8678d;
}

.tabBtn:active,.select-image-hover {
    background-color: #FC8E94;
}