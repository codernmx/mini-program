.imgBox {
        display: flex;
        justify-content: space-around;
        flex-wrap: wrap;
}

.imgBox::after {
        height: 0;
        width: 200rpx;
        content: "";
}
.upload {
        width: 100%;
        height: 350rpx;
        display: flex;
        justify-content: center;
        border: 1px gray dashed;
        border-radius: 10rpx;
        align-items: center;
        margin-bottom: 30rpx;
    }